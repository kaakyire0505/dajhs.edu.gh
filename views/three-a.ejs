   <div class="student-portal">
      <div class="portalname">
          Pupils'<span class="unit"> Portal</span>
      </div>
  
      <div class="portal-table">
          <div class="student-info" data-aos="zoom-out-right" data-aos-easing="ease-out-cubic"
          data-aos-duration="2000">
              <h5>All Class</h5>
              <hr>
              <p><i class="uil uil-user"></i> HoD: Exams Committee <br>
                  <i class="uil uil-table"></i> Class: F1 - F3 <br>
                  <i class="uil uil-phone"></i> Academics: 0200595448 <br>
                  <i class="uil uil-home"></i> Status: Professionals <br>
                  <i class="uil uil-phone"></i> Secretary: 0597639506
              </p>
              <hr>
              <div class="admin">
                <h5>Admin Use Only</h5>
                <a href="#"><i class="uil uil-padlock" id="key"></i></a>
              </div>
              <hr>
              <form action="/portal" method="post" id="adform">
              <!-- ?====control area=========== -->
              <div class="lock" data-aos="zoom-out-right" data-aos-easing="ease-out-cubic"
              data-aos-duration="2000">
                <div>
                  <label for="password"><i class="uil uil-padlock"></i> Enter Password</label>
                </div>
                <div>
                  <input type="password" name="adminpassword" placeholder="Enter password" id="adminform" required>
                </div>
                <div>
                  <button type="submit" class="btn btn-danger w-100 mt-3" id="adminbtn">Enter</button>
                </div>
                
              </form>
                <% if(typeof msg !== 'undefined'){ %>
                      <%= msg %>
               <% } %>
  
              </div>
          </div>
  
            <br><br>
              <!-- ?=====FORM THREE BEGINS==================== -->
            <div class="table" data-aos="zoom-out-right" data-aos-easing="ease-out-cubic"
            data-aos-duration="2000">
              <h6>D/A JHS 'A' FORM THREE CLASS <i class="uil uil-print" id="print"></i></h6>
              <hr>
              <table class="table table-striped table-warning" id="results-table-two">
                  <thead>
                      <tr>
                          <th>Name</th>   
                          <th>Marks Acquired</th>
                          <th>Position</th>
                          <th>Remarks</th>
                      </tr>
                      <% if(users.length > 0){ %>
                        <% users.forEach(user => { %>
                             <tr>
                                 <td><%= user.name %></td>
                                 <td><%= user.totalmarks %></td>
                                 <td><%= user.position %></td>
                                 <td><%= user.remarks %></td>
                             </tr>
                        <% }) %>
                      <% } %>                    
                  </thead>
              </table>
          </div>
          <!-- ?================FORM THREE TABLE ENDS=============== -->
          </div>
      </div>
   
  
  
